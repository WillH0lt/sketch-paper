{
  "name": "@sketchy-draw/core",
  "version": "1.0.0",
  "type": "module",
  "description": "A simple starter hello world web component written in typescript, using lit",
  "repository": "fernandopasik/hello-web-components",
  "homepage": "https://github.com/fernandopasik/hello-web-components",
  "bugs": "https://github.com/fernandopasik/hello-web-components/issues",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (https://fernandopasik.com)",
  "license": "MIT",
  "main": "dist/sketchy-draw.min.js",
  "module": "dist/sketchy-draw.min.js",
  "types": "dist/sketchy-draw.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "hello-world",
    "lit",
    "lit-element",
    "lit-html",
    "starter",
    "web-components"
  ],
  "scripts": {
    "analyze": "wca analyze src --format json --outFile custom-elements.json",
    "build": "rimraf dist artifacts && tsc -p tsconfig.build.json && copyfiles -u 2 src/assets/**/* artifacts/assets && rollup -c&& rollup -c && copyfiles -u 2 src/assets/**/* dist/assets",
    "check-types": "tsc --noEmit",
    "clean": "rimraf _site coverage dist artifacts",
    "dev": "storybook dev -p 6006",
    "docs": "jekyll serve -s docs",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext js,ts",
    "lit-analyze": "lit-analyzer src/**/* --strict",
    "prepare": "husky",
    "preversion": "npm run verify",
    "size": "size-limit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test -c playwright-ct.config.ts",
    "verify": "npm run format:check && npm run lint && npm run lit-analyze && npm run check-types && npm run test:coverage && npm run build && npm run size"
  },
  "peerDependencies": {
    "lit": "3.x.x"
  },
  "dependencies": {
    "@lastolivegames/becsy": "^0.15.5",
    "@shoelace-style/shoelace": "^2.14.0",
    "@twind/core": "^1.1.3",
    "@twind/preset-autoprefix": "^1.0.7",
    "@twind/preset-tailwind": "^1.1.4",
    "@twind/with-web-components": "^1.1.3",
    "jsondiffpatch": "^0.6.0",
    "perfect-freehand": "^1.2.2",
    "pixi-viewport": "^5.0.3",
    "pixi.js": "8.4.1",
    "strict-event-emitter": "^0.5.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@sand4rt/experimental-ct-web": "^1.47.0",
    "@size-limit/file": "^11.1.4",
    "@storybook/addon-a11y": "^8.1.11",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/web-components": "^8.1.11",
    "@storybook/web-components-vite": "^8.1.11",
    "@types/expect-puppeteer": "^5.0.6",
    "@types/jest": "^29.5.12",
    "@types/jest-environment-puppeteer": "^5.0.6",
    "@types/node": "^22.5.5",
    "@types/uuid": "^9.0.8",
    "@web/rollup-plugin-html": "^2.3.0",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "copyfiles": "^2.4.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-puppeteer": "^10.0.1",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^15.2.7",
    "lit": "^3.1.4",
    "lit-analyzer": "^2.0.3",
    "puppeteer": "^23.0.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.18.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-glslify": "^1.3.1",
    "rollup-plugin-serve": "^3.0.0",
    "size-limit": "^11.1.4",
    "storybook": "^8.1.11",
    "ts-jest": "^29.1.5",
    "ts-lit-plugin": "^2.0.2",
    "type-fest": "^4.20.1",
    "vite": "^5.3.2",
    "vite-plugin-glsl": "^1.3.0",
    "wait-on": "^8.0.0",
    "web-component-analyzer": "^2.0.0"
  },
  "overrides": {
    "glob-parent": "6.0.2"
  }
}
